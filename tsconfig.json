{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [ "es6" ],
		"sourceMap": true,
		"rootDir": "src",
		"esModuleInterop": true,
		"strict": true,
		"noImplicitReturns": false,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
	},
	"exclude": [
		"node_modules",
		".vscode-test",
		"test"
	]
}